{
    "manifest_version": 2,

    "name": "CryptoFetch",
    "description": "This extension allows you to check the values of crypto-currencies quickly.",
    "version": "1.02",
    "icons": {"128": "icon.png" },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "https://api.coinmarketcap.com/*"
    ],
    "background":
    {
        "scripts": ["js/fetch.js", "js/settings.js"]
    }
}
